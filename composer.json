{
  "name": "polkryptex/polkryptex",
  "type": "project",
  "description": "Cryptocurrency exchange platform",
  "homepage": "https://github.com/Polkryptex/Polkryptex",
  "license": "GNU-3.0",
  "keywords": [
    "cryptocurrency",
    "bitcoin"
  ],
  "require": {
    "php": "^8.0|^8.1",
    "illuminate/config": "^8",
    "illuminate/support": "^8",
    "illuminate/view": "^8",
    "illuminate/http": "^8",
    "illuminate/database": "^8",
    "illuminate/log": "^8",
    "symfony/http-client": "^6.0",
    "bramus/router": "^1.6",
    "phpmailer/phpmailer": "^6.5",
    "web-auth/webauthn-lib": "^3.3"
  },
  "require-dev": {
    "pestphp/pest": "^1.21",
    "spatie/ray": "^1.32",
    "symfony/var-dumper": "^6.0"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    },
    "classmap": [
      "app/"
    ]
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  }
}
